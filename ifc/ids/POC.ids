  <specifications>
    <specification name="Insulation Performance" ifcVersion="IFC4">
      <applicability>
        <entity>
          <name>
            <simpleValue>IfcWall</simpleValue>
          </name>
        </entity>
        <attribute>
          <name>
            <simpleValue>Name</simpleValue>
          </name>
          <value>
            <xs:restriction base="xs:string">
              <xs:pattern value=".*[Ii]nsulation.*"></xs:pattern>
            </xs:restriction>
          </value>
        </attribute>
      </applicability>
      <requirements>
        <property dataType="IFCTHERMALCONDUCTIVITYMEASURE" uri="https://identifier.buildingsmart.org/uri/demo2025/thermal-insulation-products/1.0.0/prop/ThermalConductivity">
          <propertySet>
            <simpleValue>CPset_InsulationPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>LAMBDA_DECLARED_EN12664</simpleValue>
          </baseName>
          <value>
            <xs:restriction base="xs:decimal">
              <xs:minInclusive value="0.041"></xs:minInclusive>
              <xs:maxInclusive value="0.050"></xs:maxInclusive>
            </xs:restriction>
          </value>
        </property>
        <property dataType="IFCLABEL" uri="https://identifier.buildingsmart.org/uri/demo2025/thermal-insulation-products/1.0.0/prop/ReactionToFire">
          <propertySet>
            <simpleValue>CPset_InsulationPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>REACTION_TO_FIRE</simpleValue>
          </baseName>
          <value>
            <xs:restriction base="xs:string">
              <xs:enumeration value="A1"></xs:enumeration>
              <xs:enumeration value="A2-s1d0"></xs:enumeration>
              <xs:enumeration value="A2-s2d0"></xs:enumeration>
            </xs:restriction>
          </value>
        </property>
        <property dataType="IFCMASSDENSITYMEASURE" uri="https://identifier.buildingsmart.org/uri/demo2025/thermal-insulation-products/1.0.0/prop/Density">
          <propertySet>
            <simpleValue>CPset_InsulationPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>DENSITY</simpleValue>
          </baseName>
          <value>
            <xs:restriction base="xs:decimal">
              <xs:minInclusive value="72.25"></xs:minInclusive>
              <xs:maxInclusive value="97.75"></xs:maxInclusive>
            </xs:restriction>
          </value>
        </property>
        <property dataType="IFCLABEL">
          <propertySet>
            <simpleValue>CPset_InsulationPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>WATER_VAPOUR_RESISTIVITY</simpleValue>
          </baseName>
          <value>
            <simpleValue>5.00</simpleValue>
          </value>
        </property>
        <property dataType="IFCLABEL">
          <propertySet>
            <simpleValue>CPset_InsulationPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>COMPRESSIVE_STRENGTH</simpleValue>
          </baseName>
          <value>
            <simpleValue>10</simpleValue>
          </value>
        </property>
        <property dataType="IFCREAL" uri="https://identifier.buildingsmart.org/uri/LCA/LCA/3.0/prop/GWP_total">
          <propertySet>
            <simpleValue>CPset_EpdIndicators</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>EPD_GWP_TOTAL_A1A3</simpleValue>
          </baseName>
          <value>
            <simpleValue>7.2</simpleValue>
          </value>
        </property>
      </requirements>
    </specification>

    <specification name="PVC Pipe Properties" ifcVersion="IFC4">
      <applicability>
        <entity>
          <name>
            <simpleValue>IfcPipeSegment</simpleValue>
          </name>
        </entity>
      </applicability>
      <requirements>
        <property dataType="IFCMASSDENSITYMEASURE" uri="https://identifier.buildingsmart.org/uri/etim/etim/8.0/prop/EF020000">
          <propertySet>
            <simpleValue>CPset_PipePerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>DENSITY</simpleValue>
          </baseName>
          <value>
            <simpleValue>1410</simpleValue>
          </value>
        </property>
        <property dataType="IFCLABEL">
          <propertySet>
            <simpleValue>CPset_PipePerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>COLOUR</simpleValue>
          </baseName>
          <value>
            <simpleValue>Black with blue stripes</simpleValue>
          </value>
        </property>
        <property dataType="IFCLABEL">
          <propertySet>
            <simpleValue>CPset_PipePerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>PIPE_TYPE</simpleValue>
          </baseName>
          <value>
            <simpleValue>co-extruded</simpleValue>
          </value>
        </property>
        <property dataType="IFCLABEL">
          <propertySet>
            <simpleValue>CPset_PipePerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>VACUUM_STIFF</simpleValue>
          </baseName>
          <value>
            <simpleValue>4</simpleValue>
          </value>
        </property>
        <property dataType="IFCREAL" uri="https://identifier.buildingsmart.org/uri/LCA/LCA/3.0/prop/GWP_total">
          <propertySet>
            <simpleValue>CPset_EpdIndicators</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>EPD_GWP_TOTAL_A1A3</simpleValue>
          </baseName>
          <value>
            <xs:restriction base="xs:decimal">
              <xs:minInclusive value="2.5"></xs:minInclusive>
              <xs:maxInclusive value="3.2"></xs:maxInclusive>
            </xs:restriction>
          </value>
        </property>
      </requirements>
    </specification>

    <specification name="Timber Elements Properties" ifcVersion="IFC4">
      <applicability>
        <entity>
          <name>
            <simpleValue>IfcColumn</simpleValue>
          </name>
        </entity>
      </applicability>
      <requirements>
        <property dataType="IFCLABEL">
          <propertySet>
            <simpleValue>CPset_TimberPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>STRENGTHCLASS_EN14080</simpleValue>
          </baseName>
          <value>
            <simpleValue>GL24h</simpleValue>
          </value>
        </property>
        <property dataType="IFCMASSDENSITYMEASURE">
          <propertySet>
            <simpleValue>CPset_TimberPerformance</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>DENSITY</simpleValue>
          </baseName>
          <value>
            <simpleValue>410</simpleValue>
          </value>
        </property>
        <property dataType="IFCREAL" uri="https://identifier.buildingsmart.org/uri/LCA/LCA/3.0/prop/GWP_total">
          <propertySet>
            <simpleValue>CPset_EpdIndicators</simpleValue>
          </propertySet>
          <baseName>
            <simpleValue>EPD_GWP_TOTAL_A1A3</simpleValue>
          </baseName>
          <value>
            <xs:restriction base="xs:decimal">
              <xs:minInclusive value="-400.0"></xs:minInclusive>
              <xs:maxInclusive value="-300.0"></xs:maxInclusive>
            </xs:restriction>
          </value>
        </property>
      </requirements>
    </specification>
  </specifications>
</ids>